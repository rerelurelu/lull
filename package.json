{
  "name": "lull",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "panda codegen --clean",
    "setup": "lefthook install",
    "dev": "next dev --turbo",
    "dev:debug": "next dev -p 3000",
    "prebuild": "bun run generate:link-meta && bun run generate:feed",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "fmt:check": "bunx @biomejs/biome check --write ./src",
    "test": "vitest",
    "generate:link-meta": "bun run scripts/generateLinkMeta.ts",
    "generate:feed": "bun run scripts/generateFeed.ts",
    "link-meta:stats": "bun -e \"import('./scripts/generateLinkMeta.ts').then(m => m.logMetadataStats())\"",
    "feed:stats": "bun -e \"import('./scripts/generateFeed.ts').then(m => m.logFeedStats())\"",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "dependencies": {
    "@ark-ui/react": "^4.4.4",
    "microcms-js-sdk": "^3.1.2",
    "next": "^16.0.7",
    "react": "19.2.1",
    "react-dom": "19.2.1",
    "rehype-highlight": "^7.0.1",
    "rehype-parse": "^9.0.1",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "^10.0.1",
    "unified": "^11.0.5",
    "yet-another-react-lightbox": "^3.27.0"
  },
  "devDependencies": {
    "@biomejs/biome": "2.2.4",
    "@pandacss/dev": "^1.7.2",
    "@types/node": "^20",
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2",
    "babel-plugin-react-compiler": "^19.1.0-rc.3",
    "dev3000": "^0.0.51",
    "feed": "^5.1.0",
    "react-icons": "^5.3.0",
    "typescript": "^5",
    "vitest": "^2.1.5",
    "storybook": "^10.1.11",
    "@storybook/nextjs-vite": "^10.1.11",
    "@storybook/addon-a11y": "^10.1.11",
    "@storybook/addon-docs": "^10.1.11",
    "@storybook/addon-onboarding": "^10.1.11",
    "vite": "^7.3.0"
  },
  "overrides": {
    "@types/react": "19.2.2",
    "@types/react-dom": "19.2.2"
  }
}
